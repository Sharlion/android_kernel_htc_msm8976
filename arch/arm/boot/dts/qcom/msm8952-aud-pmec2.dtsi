		pinctrl-0 = <&pri_tlmm_lines0_sus &pri_tlmm_lines1_sus>;
		pinctrl-1 = <&pri_tlmm_lines0_act &pri_tlmm_lines1_act>;

		qcom,headset-jack-type-NC;
		qcom,audio-routing =
